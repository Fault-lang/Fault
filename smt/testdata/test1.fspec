spec test1;
			const a = 2.3;
			const b = 2;

			def foo = flow{
				buzz: new bar,
				fizz: func{
					a + buzz.a -> buzz.b;
				},
				fizz2: func{
					buzz.a - b -> buzz.b;
				},
				fizz3: func{
					buzz.b + b -> buzz.a;
				},
			};

			def bar = stock{
				a: 10,
				b: 20,
			};

			for 5 run{
				test = new foo;
				test.fizz | test.fizz2;
				test.fizz3;
			};